<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="parent" alignment="BASELINE_LEFT" spacing="16.0" styleClass="card" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ntnu.idatt1002.k2_2.mitodo.view.components.SubProject">
   <children>
      <Label fx:id="projectName" alignment="TOP_CENTER" styleClass="h2" text="PROJECT_NAME">
         <font>
            <Font />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#handleGoToProjectClick" text="Go To Project">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="8.0" left="32.0" right="32.0" top="8.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</HBox>
